<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title - @ViewBag.SiteName</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />

    <script type="text/javascript">
        function setUrl() {
            history.replaceState('Object', 'Title', '@ViewBag.Id');
        }
    </script>
    
    <script src="@Url.Content("~/Scripts/jquery-2.1.3.min.js")" type="text/javascript"></script>

    @RenderSection("Head", required: false)
    
    

</head>
<body onload="setUrl();  @RenderSection("OnLoad", required: false)">
    <div class="page" style="position:relative">
@RenderBody()
</div>
</body>
</html>
